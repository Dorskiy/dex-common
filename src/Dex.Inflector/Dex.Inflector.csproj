<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageTags>dex, pluralize, singularize</PackageTags>

  </PropertyGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'net462' ">
