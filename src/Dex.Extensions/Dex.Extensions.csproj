<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
      <LangVersion>Latest</LangVersion>
      <Nullable>enable</Nullable>
      <PackageTags>dex; extensions</PackageTags>
      <RepositoryUrl>https://github.com/dex-it/dex-common</RepositoryUrl>
      <Copyright>dex</Copyright>
      <PackageProjectUrl>https://github.com/dex-it/dex-common/projects/1</PackageProjectUrl>
      <RepositoryType>github</RepositoryType>
      <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
      <EnableNETAnalyzers>true</EnableNETAnalyzers>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.NetAnalyzers" Version="5.0.1">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>
</Project>
